{
  "manifest_version":2,
  "name":"YouTube",
  "version":"1.0",
  "description":"Spend some time on Youtube",
  "icons":{
       "128":"youtube.png"
  },
  "browser_action":{
    "default_icon":"youtube.png",
    "default_popup":"popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs", "<all_urls>", "http://192.168.0.106/",
    "http://*/*", "https://*/*"
  ]




}